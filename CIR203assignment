      "source": [
        "import numpy as np\n",
        "\n",
        "transactions = np.array([\n",
        "    [1200, 1350, 1600, 1450, 1700, 1800],   # Branch 1\n",
        "    [1000, 1100, 1150, 1200, 1300, 1250],   # Branch 2\n",
        "    [900,  950,  1000, 1050, 1100, 1200],   # Branch 3\n",
        "    [1500, 1600, 1700, 1650, 1750, 1900]    # Branch 4\n",
        "])\n",
        "\n",
        "print(\"Transaction Volume Array (4 branches × 6 months):\")\n",
        "print(transactions)\n",
        "\n",
        "branch_totals = np.sum(transactions, axis=1)\n",
        "print(\"Total Transactions Per Branch:\")\n",
        "print(branch_totals)\n",
        "\n",
        "highest_branch_index = np.argmax(branch_totals)\n",
        "print(f\"Branch with Highest Transactions: Branch {highest_branch_index + 1}\")\n",
        "\n",
        "average_volume = np.mean(transactions)\n",
        "print(\"Average Monthly Transaction Volume Across All Branches:\")\n",
        "print(average_volume)\n",
        "\n",
        "reshaped = transactions.reshape(3, 8)\n",
        "\n",
        "print(\"Reshaped Array (3 × 8):\")\n",
        "print(reshaped)\n",
        "\n",
        "print(\"\\nImplications of Reshaping:\")\n",
        "print(\"- The data values remain the same, but their positions change.\")\n",
        "print(\"- Reshaping does NOT combine, delete, or modify values; it only reorganizes them.\")\n",
        "print(\"- The new shape must contain the same total number of elements (4×6 = 24 = 3×8).\")"
      ]
